{
  "description": "Checking untested versions",
  "manifest_version": 2,
  "name": "YouTrack Builds Checker",
  "version": "1.0.2.0",
  "homepage_url": "https://github.com/cernyjan/YouTrack-BuildsChecker",
  "icons": {
    "48": "icons/checker-48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/checker-48.png",
      "32": "icons/checker-48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://youtrack/*", "https://youtrack.quadient.group/*", "*://*/youtrack*"],
      "js": ["jquery-3.2.1.min.js", "YouTrack-BuildsChecker.js"]
    }
  ]
}
